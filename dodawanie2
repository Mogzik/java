import javax.swing.JOptionPane;

public class z5 {
    public static void main(String[] args) {

double wynik;
String input = JOptionPane.showInputDialog("Podaj 1 liczbe do sumowania");
double l1 = Integer.parseInt(input);
String input1 = JOptionPane.showInputDialog("Podaj 2 liczbe do sumowania");
double l2 = Integer.parseInt(input1);
wynik=l1+l2;
JOptionPane.showMessageDialog(null,"Tw√≥j wynik wynosi "+wynik);
    }
}
